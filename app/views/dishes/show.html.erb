<div class="page-header">
  <h1>Dish #<%= @dish.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @dish.name %></dd>

  <dt>Cuisine</dt>
  <dd><%= @cuisine.name %></dd>

  <dt>Favorite</br>(by location and user)</dt>
    <dd>
    <%  @favorites.each do |favorite| %>
      <li> <%= Venue.find(favorite.venue_id).name %> / <%= User.find(favorite.user_id).username %>
    <% end %>
    </dd>
</dl>
